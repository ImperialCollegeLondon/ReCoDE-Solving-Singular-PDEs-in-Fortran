close all
clear all

X = readmatrix('1.dat');
w = width(X(1,:))
h = height(X(:,1))

x = X(2:h,1)
t = X(1,2:w)
sol = X(2:h,2:w)

u = ex

% Define function and time range
% Create animated plot
figure;
for k = 1:length(t)
    plot(x, sol(:, k));
    xlabel('x');
    ylabel('y');
    %title(sprintf('t = %0.1f', t(k)));
    %ylim([-1.5, 1.5]);
    pause(1);
end